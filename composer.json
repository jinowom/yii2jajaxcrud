  "type": "yii2-extension",
  "keywords": [
    "yii2",
    "extension",
    "ajax",
    "crud",
    "gii",
    "template",
    "database"
  ],
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "jinowom",
      "email": "chareler@g163.com",
      "homepage": "https://github.com/jinowom?tab=repositories"
    }
  ],
  "support": {
    "issues": "https://github.com/jinowom/yii2jajaxcrud/issues?state=open",
    "source": "https://github.com/jinowom/yii2jajaxcrud"
  },
  "require": {
    "php": ">=7.1 || ^8.2",
    "yiisoft/yii2": "^2.0.6",
    "yiisoft/yii2-gii": "^2.1.0",
    "yiisoft/yii2-bootstrap": "^2.0.0",
    "kartik-v/yii2-krajee-base": "^2.0|^3.0.5",
    "kartik-v/yii2-grid": "^3.3.4",
    "kartik-v/yii2-detail-view": "^1.8.2",
    "kartik-v/yii2-date-range": "^1.7.1",
    "kartik-v/yii2-mpdf": "^1.0.5",
    "kartik-v/yii2-editable": "^1.7.8",
    "kartik-v/yii2-widget-activeform": "^1.5.8",
    "jinowom/yii2jtools": "1.0.2"
  },
  "autoload": {
    "psr-4": {
      "jinowom\\jajaxcrud\\": "src/"
    }
  },
  "extra": {
    "bootstrap": "jinowom\\jajaxcrud\\Bootstrap"
  }
}
